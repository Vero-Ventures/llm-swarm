{
    "name": "llm-swarm",
    "displayName": "LLM Swarm",
    "description": "LLM-Swarm Project",
    "version": "1.0.0",
    "preview": true,
    "serverInfo": {
        "name": "LLM Swarm",
        "module": "llm-swarm"
    },
    "publisher": "<my-publisher>",
    "author": "Vero Ventures",
    "license": "ISC",
    "homepage": "https://github.com/Vero-Ventures/llm-swarm.git",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Vero-Ventures/llm-swarm.git.git"
    },
    "bugs": {
        "url": "git+https://github.com/Vero-Ventures/llm-swarm.git/issues"
    },
    "galleryBanner": {
        "color": "#1e415e",
        "theme": "dark"
    },
    "keywords": [
        "AI",
        "LLM"
    ],
    "engines": {
        "vscode": "^1.78.0"
    },
    "categories": [
        "Programming Languages",
        "Linters",
        "Formatters"
    ],
    "extensionDependencies": [
        "ms-python.python"
    ],
    "capabilities": {
        "virtualWorkspaces": {
            "supported": false,
            "description": "Virtual Workspaces are not supported with LLM Swarm."
        }
    },
    "activationEvents": [
        "onLanguage:python",
        "workspaceContains:*.py"
    ],
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool source-map --config ./webpack.config.js",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "format-check": "prettier --check 'src/**/*.ts' 'build/**/*.yml' '.github/**/*.yml'",
        "test": "node ./out/test/runTest.js",
        "eslint": "eslint --config eslint.config.js --no-ignore src/**/*.{js,jsx,ts,tsx}",
        "vsce-package": "vsce package -o llm-swarm.vsix"
    },
    "contributes": {
        "configuration": {
            "properties": {
                "llm-swarm.args": {
                    "default": [],
                    "description": "Arguments passed in. Each argument is a separate item in the array.",
                    "items": {
                        "type": "string"
                    },
                    "scope": "resource",
                    "type": "array"
                },
                "llm-swarm.path": {
                    "default": [],
                    "description": "When set to a path to llm-swarm binary, extension will use that. NOTE: Using this option may slowdown server response time.",
                    "scope": "resource",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "llm-swarm.importStrategy": {
                    "default": "useBundled",
                    "description": "Defines where `llm-swarm` is imported from. This setting may be ignored if `llm-swarm.path` is set.",
                    "enum": [
                        "useBundled",
                        "fromEnvironment"
                    ],
                    "enumDescriptions": [
                        "Always use the bundled version of `llm-swarm`.",
                        "Use `llm-swarm` from environment, fallback to bundled version only if `llm-swarm` not available in the environment."
                    ],
                    "scope": "window",
                    "type": "string"
                },
                "llm-swarm.interpreter": {
                    "default": [],
                    "description": "When set to a path to python executable, extension will use that to launch the server and any subprocess.",
                    "scope": "resource",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "llm-swarm.showNotifications": {
                    "default": "off",
                    "description": "Controls when notifications are shown by this extension.",
                    "enum": [
                        "off",
                        "onError",
                        "onWarning",
                        "always"
                    ],
                    "enumDescriptions": [
                        "All notifications are turned off, any errors or warning are still available in the logs.",
                        "Notifications are shown only in the case of an error.",
                        "Notifications are shown for errors and warnings.",
                        "Notifications are show for anything that the server chooses to show."
                    ],
                    "scope": "machine",
                    "type": "string"
                }
            }
        },
        "commands": [
            {
                "title": "Restart Server",
                "category": "LLM Swarm",
                "command": "llm-swarm.restart"
            },
            {
                "title": "Improve Code",
                "category": "LLM Swarm",
                "command": "llm-swarm.improveCode"
            }
        ]
    },
    "dependencies": {
        "@vscode/python-extension": "^1.0.5",
        "fs-extra": "^11.2.0",
        "vscode-languageclient": "^9.0.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/glob": "^8.1.0",
        "@types/node": "20.x",
        "@types/vscode": "1.89.0",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.8.0",
        "@vscode/test-electron": "^2.3.8",
        "@vscode/vsce": "^2.22.0",
        "eslint": "^8.57.0",
        "glob": "^10.3.10",
        "globals": "^15.3.0",
        "prettier": "^3.2.5",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    }
}
